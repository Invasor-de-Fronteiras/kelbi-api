{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["xo", "xo-typescript/space"],
    "overrides": [
        {
            "extends": [
                "xo-typescript/space"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "new-cap": "off"
    }
}
